{% extends 'layouts/page.njk' %}

{% block content %}
  {{ appHeading({
    title: heading,
    caption: "File upload complete"
  }) }}

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <div class="govuk-body" data-testid="app-page-body">

        <p>
          The file was uploaded and scanned. No issues were found and it was uploaded to S3.
        </p>
        <p>Here's the S3 metadata:</p>
        <pre style='overflow: clip;'>{{ metadata }}</pre>

        <div style='height: 50em;'></div>
      </div>
    </div>
  </div>


{% endblock %}
